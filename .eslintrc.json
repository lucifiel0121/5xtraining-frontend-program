{
   "extends": ["react-app", "airbnb", "prettier", "prettier/react"],
   "plugins": ["prettier", "react"],
   "rules": {
      "react/prop-types": 0,
      "prettier/prettier": "error",
      "react/state-in-constructor": 0,
      "react/jsx-filename-extension": [0]
   },
   "parser": "babel-eslint",
   "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
         "jsx": true
      }
   },
   "env": {
      "es6": true,
      "browser": true,
      "node": true
   },
   "settings": {
      "react": {
         "version": "detect"
      }
   }
}
